#!/usr/bin/env mkb
# Builder mkb file for the java portion of the s3eAdWhirl extension on android
platform JAVA

files
{
    (source/android)
    s3eAdWhirl.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

librarypath "$MARMALADE_ROOT/extensions/s3eAdWhirl/GoogleAdMobAdsSdk-4.3.1.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdWhirl/AdWhirlSDK_Android_3.1.1.jar"

option output-name=lib/android/s3eAdWhirl.jar
